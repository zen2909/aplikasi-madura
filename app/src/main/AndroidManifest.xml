<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ELearning_Bahasa_madura"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ELearning_Bahasa_madura">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.Terjemahan"/>
        <activity android:name=".view.TingkatBahasa"/>
        <activity android:name=".view.Pelafalan"/>
        <activity android:name=".view.Evaluasi"/>
        <activity android:name=".view.evalTerjemahan"/>
        <activity android:name=".view.evalTingkatBahasa"/>
        <activity android:name=".view.evalPelafalan"/>
    </application>

</manifest>